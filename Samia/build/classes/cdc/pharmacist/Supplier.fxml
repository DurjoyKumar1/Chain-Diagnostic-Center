<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.geometry.Insets?>
<?import javafx.scene.control.TableColumn?>
<?import javafx.scene.control.TableView?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.layout.BorderPane?>
<?import javafx.scene.text.Font?>
<?import javafx.scene.text.Text?>

<BorderPane prefHeight="500.0" prefWidth="700.0" xmlns="http://javafx.com/javafx/20.0.2" xmlns:fx="http://javafx.com/fxml/1" fx:controller="cdc.pharmacist.SupplierController">
   <top>
      <!-- Include menu.fxml here -->
      <fx:include source="menu.fxml" />
   </top>
   <center>
      <AnchorPane prefHeight="500.0" prefWidth="700.0">
         <children>
            <BorderPane prefHeight="500.0" prefWidth="700.0">
               <top>
                  <Text fill="#021ffc" stroke="#1bd057" strokeType="OUTSIDE" text="Supplier List" textAlignment="CENTER" BorderPane.alignment="CENTER">
                     <font>
                        <Font size="24.0" />
                     </font>
                     <BorderPane.margin>
                        <Insets top="10.0" />
                     </BorderPane.margin>
                  </Text>
               </top>
               <center>
                  <TableView prefHeight="450.0" prefWidth="700.0" BorderPane.alignment="TOP_CENTER">
                     <columns>
                        <TableColumn maxWidth="350.0" prefWidth="75.0" text="Name" />
                        <TableColumn maxWidth="350.0" prefWidth="75.0" text="Address" />
                     </columns>
                     <columnResizePolicy>
                        <TableView fx:constant="CONSTRAINED_RESIZE_POLICY" />
                     </columnResizePolicy>
                     <BorderPane.margin>
                        <Insets top="10.0" />
                     </BorderPane.margin>
                  </TableView>
               </center>
            </BorderPane>
         </children>

      </AnchorPane>
   </center>
</BorderPane>
