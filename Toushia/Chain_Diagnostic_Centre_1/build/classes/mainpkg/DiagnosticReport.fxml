<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Label?>
<?import javafx.scene.control.RadioButton?>
<?import javafx.scene.control.TableColumn?>
<?import javafx.scene.control.TableView?>
<?import javafx.scene.effect.InnerShadow?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.text.Font?>

<AnchorPane id="AnchorPane" prefHeight="592.0" prefWidth="799.0" xmlns="http://javafx.com/javafx/8.0.171" xmlns:fx="http://javafx.com/fxml/1" fx:controller="mainpkg.DiagnosticReportController">
   <children>
      <Label layoutX="238.0" layoutY="35.0" prefHeight="52.0" prefWidth="294.0" text="Diagnostic Report" textFill="#168008">
         <font>
            <Font size="32.0" />
         </font>
      </Label>
      <Label layoutX="40.0" layoutY="107.0" text="Record Files of patients " textFill="#2ab015">
         <font>
            <Font size="15.0" />
         </font>
      </Label>
      <TableView fx:id="RecordFilesTable" layoutX="40.0" layoutY="137.0" prefHeight="239.0" prefWidth="677.0">
         <columns>
            <TableColumn fx:id="SerialNoColumn" prefWidth="102.0" text="Serial No" />
            <TableColumn fx:id="PatientNameColumn" prefWidth="148.0" text="Patient Name " />
            <TableColumn fx:id="AgeColumn" minWidth="0.0" prefWidth="95.0" text="Age" />
            <TableColumn fx:id="TestNameColumn" prefWidth="131.0" text="Test Name" />
            <TableColumn fx:id="SampleTypeColumn" prefWidth="142.0" text="Sample Type (if needed)" />
         </columns>
         <effect>
            <InnerShadow choke="0.21" color="#168008" radius="21.385" width="66.54" />
         </effect>
      </TableView>
      <Label layoutX="40.0" layoutY="420.0" text="Reports have been made from findings from the Table" textFill="#2ab015">
         <font>
            <Font size="15.0" />
         </font>
      </Label>
      <RadioButton fx:id="YeSReportMadeButton" layoutX="40.0" layoutY="456.0" mnemonicParsing="false" text="yes" textFill="#3552b2">
         <font>
            <Font size="16.0" />
         </font>
      </RadioButton>
      <RadioButton fx:id="NoReportMadeButton" layoutX="40.0" layoutY="492.0" mnemonicParsing="false" text="No" textFill="#3552b2">
         <font>
            <Font size="16.0" />
         </font>
      </RadioButton>
   </children>
</AnchorPane>

